# Our forward zone
zone "home" {
 type master;
 file "/etc/bind/db.home";
 };

# Our reverse Zone 
# Server IP 192.168.1.5 
zone "2.168.192.in-addr.arpa" {
  type master;
  file "/etc/bind/db.home.inv";
 };
